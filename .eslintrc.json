{
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "eqeqeq": 0,
    "curly": 2,
    "quotes": [1, "single"]
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "globals": {
    "window": true,
    "document": true,
    "assert": true,
    "simulant": true,
    "describe": true,
    "it": true,
    "beforeEach": true,
    "before": true,
    "after": true,
    "afterEach": true,
    "Combobo": true
  }
}
