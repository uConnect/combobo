{
  "$schema": "https://swc.rs/schema.json",
  "jsc": {
    "parser": {
      "syntax": "ecmascript",
      "optionalChaining": true,
      "nullishCoalescing": true,
      "classProperty": true,
      "exportDefaultFrom": true
    },
    "target": "es2016",
    // "loose": true,
    "externalHelpers": false,
    // Requires v1.2.50 or upper and requires target to be es2016 or upper.
    "keepClassNames": true
  },
  "module": {
    "type": "umd",
    "globals": {},
    "lazy": true
  },
  "minify": false
}